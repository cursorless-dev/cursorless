{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "moduleDetection": "force",
    "target": "es6",
    "lib": ["es2022"],
    "sourceMap": true,
    "declarationMap": true,
    "resolveJsonModule": true,
    "composite": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "strict": true
  }
}
