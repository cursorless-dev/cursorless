{
  "name": "@cursorless/cheatsheet",
  "version": "0.1.0",
  "description": "Core cheatsheet react component",
  "main": "./out/index.js",
  "scripts": {
    "test": "jest",
    "compile": "esbuild ./src/index.ts --sourcemap --format=esm --bundle --packages=external --outfile=./out/index.js",
    "watch": "tsc --build --watch",
    "clean": "rm -rf ./out tsconfig.tsbuildinfo ./dist ./build",
    "watch:esbuild": "pnpm compile --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "types": "./out/index.d.ts",
  "exports": {
