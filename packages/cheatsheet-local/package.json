{
  "name": "@cursorless/cheatsheet-local",
  "version": "0.1.0",
  "private": true,
  "description": "Thin wrapper around the cheatsheet for showing local cheatsheet",
  "main": "./out/index.js",
  "scripts": {
    "compile": "tsc --build",
    "watch": "tsc --build --watch",
    "webpack": "pnpm -w compile && env NODE_OPTIONS=--loader=ts-node/esm webpack --config ./src/webpack.config.ts",
    "build": "pnpm build:prod",
    "build:dev": "pnpm webpack --mode=development",
    "build:prod": "pnpm webpack --mode=production --node-env=production",
    "clean": "rm -rf ./out tsconfig.tsbuildinfo ./dist ./build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "types": "./out/index.d.ts",
  "exports": {
    ".": {
      "cursorless:bundler": "./src/index.ts",
      "default": "./out/index.js"
    }
  },
  "dependencies": {
    "@cursorless/cheatsheet": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@effortlessmotion/html-webpack-inline-source-plugin": "1.0.3",
    "@tailwindcss/postcss": "4.0.13",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.2.0",
    "@types/jest": "29.5.14",
    "@types/node": "20.16.0",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/webpack": "5.28.5",
    "@webpack-cli/generators": "3.0.7",
    "autoprefixer": "10.4.21",
    "css-loader": "7.1.2",
    "html-webpack-plugin": "5.6.3",
    "jest": "29.7.0",
    "postcss": "8.5.3",
    "postcss-loader": "8.1.1",
    "style-loader": "4.0.0",
    "tailwindcss": "4.0.12",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "^5.8.2",
    "webpack": "5.98.0",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.0"
  },
  "postcss": {
    "plugins": {
      "@tailwindcss/postcss": {},
      "autoprefixer": {}
    }
  },
  "type": "module"
}
