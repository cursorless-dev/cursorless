{
  "rules": {
    "@typescript-eslint/no-restricted-imports": [
      "error",
      {
        "patterns": [
          {
            "group": [
              "@cursorless/*",
              "!@cursorless/common",
              "!@cursorless/vscode-common",
              "../../*"
