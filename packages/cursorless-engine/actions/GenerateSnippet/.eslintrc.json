{
  "rules": {
    "@typescript-eslint/no-restricted-imports": [
      "error",
      {
        "patterns": [
          {
            "group": ["@cursorless/*", "!@cursorless/common", "../../../*"],
            "message": "Cursorless engine has restricted dependencies"
          }
        ],
        "paths": [
          {
            "name": "vscode",
            "message": "Cursorless engine shouldn't depend on vscode"
          }
        ]
      }
    ]
  }
}
